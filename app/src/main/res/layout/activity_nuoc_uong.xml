        app:title="NƯỚC UỐNG"
        app:titleTextAppearance="@style/Toolbar.TitleText"
        app:titleTextColor="@color/white"></android.support.v7.widget.Toolbar>

    <ListView
        android:id="@+id/list"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </ListView>
</LinearLayout>

