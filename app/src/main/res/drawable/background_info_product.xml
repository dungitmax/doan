<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#5998e6"/>
            <corners android:radius="10dp"/>
            <stroke android:color="#25dfcd" android:width="1dp"/>
        </shape>
    </item>

    <item >
        <shape>
            <solid android:color="#1a4fe2"/>
            <corners android:radius="10dp"/>
            <stroke android:color="#0fed4a" android:width="1dp"/>
        </shape>
    </item>
</selector>